{
    "name": "seattledigital/amtrak-materialicons",
    "description": "A package to easily make use of Materialicons in your Laravel Amtrak views.",
    "keywords": [
        "Amtrak",
        "Materialicons",
        "Laravel"
    ],
    "homepage": "https://github.com/seattledigital/amtrak-materialicons",
    "license": "MIT",
    "authors": [
        {
            "name": "William White Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "amtrak-ui-kit/amtrak-icons": "^1.0",
        "illuminate/support": "^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "AmtrakUI\\Materialicons\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AmtrakUI\\Materialicons\\AmtrakMaterialiconsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
